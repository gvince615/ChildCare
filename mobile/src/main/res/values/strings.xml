  <!--regex-->
  <string name="name_validation" tools:ignore="TypographyDashes">^[a-zA-Z_/`/\'-.]+(?! and )( [a-zA-Z_/`/\'-.]+)*$</string>
  <string name="date_validation_regex" tools:ignore="TypographyDashes">^(0[1-9]|1[0-2])/(0[1-9]|1\\d|2\\d|3[01])/(19|20)\\d{2}$</string>
  <string name="phone_validation" tools:ignore="TypographyDashes">^([0-9]{3})-([0-9]{3})-([0-9]{4})$|^(([+]{1}[0-9]{2}|0)[0-9]{9})$</string>
  <string name="us_2digit_state_validation" tools:ignore="TypographyDashes">[A-Z]*</string>
  <string name="city_name_validation" tools:ignore="TypographyDashes">|^[a-zA-Z0-9_]+( [a-zA-Z0-9,.;:\'-]+)*$</string>
  <string name="address_validation" tools:ignore="TypographyDashes">|[a-zA-Z0-9~@#$^*_+=|\\,.?: -]*$</string>
  <string name="us_zip_validation" tools:ignore="TypographyDashes">([0-9]{5}|)</string>
  <string name="currency_validation" tools:ignore="TypographyDashes">[.,$0-9]*</string>
  <string name="time_validation" tools:ignore="TypographyDashes">^([01]?[0-9]|2[0-3]):[0-5][0-9]*$</string>
  <string name="email_validation" tools:ignore="TypographyDashes">^[a-zA-Z0-9.!#$%*+/=?^_`{|}~-]+@[a-zA-Z0-9](?:[a-zA-Z0-9-]{0,61}[a-zA-Z0-9])?(?:\.[a-zA-Z0-9](?:[a-zA-Z0-9-]{0,61}[a-zA-Z0-9])?)*$</string>

